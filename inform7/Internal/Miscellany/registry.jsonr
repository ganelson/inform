<registry-holding> ::= {
	"author": string,
	"title": string,
	?"standard-installation": boolean,
	?"visible": boolean,
	?"forum-thread": number,
	?"directory-format": boolean,
	"version": string,
	"summary": string,
	"time": string
}

<registry-subsection> ::= {
	"title": string,
	"holdings": [ <registry-holding>* ]
}

<registry-section> ::= {
	"title": string,
	"leafname": string,
	"subsections": [ <registry-subsection>* ]
}

<registry-metadata> ::= {
	"sections": [ <registry-section>* ]
}
